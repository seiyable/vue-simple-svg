    "url-loader": "^1.1.1",
    "vue": "^2.5.16",
    "vue-loader": "^13.7.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "cssom": "^0.3.2"
  }
}
